<?xml version="1.0"?>
<!-- Kommentar -->

<robot name="homer">
	<link name="world"/>

  	<joint name="base_joint" type="fixed">
    		<parent link="world"/>
    		<child link="base_link"/>
    		<origin xyz="0 0 0" rpy="0 0 0" />
  	</joint>

	<link name="base_link">
   		<inertial>
      			<origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      			<mass value="0.0"/>
      			<inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    		</inertial>
		
		<visual>
			<geometry>
				<mesh filename = "package://elissa3_homer/homer_parts/Base_End.stl" scale="0.01 0.01 0.01"/> <!--scale from mm to m should be 0.001--> <!--scale="0.01 0.01 0.01"--> 
			</geometry>
				
			<!--origin - the reference frame of the visual element with respect to the reference frame of the link -->
			<!--rpy -r is roll, p-pitch and y-yaw (Eulerwinkel) -->
			<!--xyz - x,y,z -offsets -->
			<origin xyz="0 0 0" rpy="0 0 0" />
			<material name="yellow">
				<color rgba="1 1 0 1"/>
			</material>
		</visual>
		
    		<collision>
      			<origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      			<geometry>
        			<mesh filename = "package://elissa3_homer/homer_parts/Base_End.stl" scale="0.01 0.01 0.01"/>
      			</geometry>
    		</collision>	
	</link>
	
	<joint name="joint_1" type="revolute"> <!-- maybe continous -->
		<parent link="base_link"/>
		<child link="first_link"/>
		<origin xyz="0 0 0.4" rpy="0 0.7854 0"/>
		<axis xyz="0 0 1"/>
		<limit effort="300" velocity="2.0" lower="-6" upper="6"/>
		<dynamics damping="0.1"/>
	</joint>
	
	<link name="first_link">
	   	<inertial>
      			<origin xyz="0.07 0 -0.069282" rpy="0 -0.7854 0" />
      			<mass value="0.0"/>
      			<inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    		</inertial>
    		
		<visual>
			<geometry>
				<mesh filename = "package://elissa3_homer/homer_parts/Mid.stl" scale="0.01 0.01 0.01"/> <!--scale="0.01 0.01 0.01"--> 
			</geometry>
			<origin xyz="0.07 0 -0.069282" rpy="0 -0.7854 0" />
			<material name="blue">
				<color rgba="0 0 1 1"/>
			</material>
		</visual>
		<collision>
      			<origin xyz="0.07 0 -0.069282" rpy="0 -0.7854 0" />
      			<geometry>
        			<mesh filename = "package://elissa3_homer/homer_parts/Mid.stl" scale="0.01 0.01 0.01"/> <!--scale="0.01 0.01 0.01"-->
      			</geometry>
    		</collision>
	</link>
	
	<joint name="joint_2" type="revolute"> <!-- maybe continous -->
		<parent link="first_link"/>
		<child link="second_link"/>
		<origin xyz="-0.2121 0 0.2121" rpy="0 0 0"/>
		<axis xyz="0 0 1"/>
		<limit effort="300" velocity="2.0" lower="-6" upper="6"/>
		<dynamics damping="0.1"/>
	</joint>
	
	<link name="second_link">
	
		<inertial>
      			<origin xyz="0.07 0 -0.069282" rpy="0 -0.7854 0" />
      			<mass value="0.0"/>
      			<inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    		</inertial>
		<visual>
			<geometry>
				<mesh filename = "package://elissa3_homer/homer_parts/Mid.stl" scale="0.01 0.01 0.01"/> <!--scale="0.01 0.01 0.01"--> 
			</geometry>
			<origin xyz="0.07 0 -0.069282" rpy="0 -0.7854 0" />
			<material name="yellow">
				<color rgba="1 1 0 1"/>
			</material>
		</visual>
		<collision>
      			<origin xyz="0.07 0 -0.069282" rpy="0 -0.7854 0" />
      			<geometry>
        			<mesh filename = "package://elissa3_homer/homer_parts/Mid.stl" scale="0.01 0.01 0.01"/> <!--scale="0.01 0.01 0.01"-->
      			</geometry>
    		</collision>
	</link>
	
	<transmission name="trans_1">
		<type> transmission_interface/SimpleTransmission</type>
		<joint name="joint_1">
			<hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
		</joint>
		<actuator name ="base_link_motor">"
			<hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
			<mechanicalReduction>1</mechanicalReduction>
		</actuator>
	</transmission>
	
		<transmission name="trans_2">
		<type> transmission_interface/SimpleTransmission</type>
		<joint name="joint_2">
			<hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
		</joint>
		<actuator name ="base_link_motor">"
			<hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
			<mechanicalReduction>1</mechanicalReduction>
		</actuator>
	</transmission>
		
	<gazebo>
		<plugin name="control" filename = "libgazebo_ros_control.so">
			<robotNamespace>/</robotNamespace>
		</plugin>
	</gazebo>
	
	<gazebo reference="base_link">
		<selfCollide>true</selfCollide>
	</gazebo>
	
	<gazebo reference="first_link">
		<selfCollide>true</selfCollide>
	</gazebo>
	
	<gazebo reference="second_link">
		<selfCollide>true</selfCollide>
	</gazebo>
	
	
</robot> 
